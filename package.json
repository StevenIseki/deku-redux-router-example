    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "ava": "^0.11.0",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-react-jsx": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "css-block-loader": "^1.0.1",
    "css-loader": "^0.15.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "postcss-loader": "^0.7.0",
    "postcss-modules-values": "^1.1.1",
    "style-loader": "^0.13.0",
