{
  "name": "deku-redux-router-example",
  "version": "1.0.0",
  "description": "An example of deku 2.0 with redux and history.",
  "main": "index.js",
  "scripts": {
    "dist": "webpack --config webpack.config.js",
    "build": "npm run clean && npm run dist",
    "clean": "rm public/bundle.js && rm public/style.css || true",
    "start": "node serverDev.js",
    "start:prod": "NODE_ENV=production node server.js",
    "test": "ava tests/*.js"
  },
  "repository": {
